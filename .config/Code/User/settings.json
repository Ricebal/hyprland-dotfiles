{
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "editor.fontFamily": "'Mononoki Nerd Font', monospace",
    "terminal.integrated.fontFamily": "monospace",
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorCustomizations": {
        "terminal.background": "#00000000"
    },
    "workbench.settings.applyToAllProfiles": [
        "workbench.colorCustomizations"
    ],
    "editor.semanticTokenColorCustomizations": {
        "[functional-purple]": {
            "enabled": true,
            "rules": {
                "class": "#f832c7"
            }
        },
        "[City Lights]": {
            "enabled": true,
            "rules": {
                "class": "#0099ff",
                "keyword": "#9962ff",
                "parameter": "#e9a1ff"
            }
        }
    },
    "editor.tokenColorCustomizations": {
        "[Dark Green Jungle]": {
            "types": "#6c9659b6",
            "functions": "#ddb467",
            "variables": "#6cad16"
        },
        "[Dark Magic Frankenstein]": {
            "types": "#679218"
        },
        "[functional-purple]": {
            "types": "#fc87df",
            "variables": "#ffffff",
            "functions": "#60aee2"
        },
        "[City Lights]": {
            "variables": "#ffffff",
            "types": "#6ec5ff"
        }
    },
    "editor.lineNumbers": "relative",
    "workbench.startupEditor": "none",
    "diffEditor.ignoreTrimWhitespace": false,
    "window.newWindowProfile": "Default",
    "editor.minimap.enabled": false,
    "window.commandCenter": false,
    "workbench.activityBar.location": "top",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "githubPullRequests.pullBranch": "never",
    "window.menuBarVisibility": "toggle",
    "workbench.editor.editorActionsLocation": "hidden",
    "window.customTitleBarVisibility": "never",
    "chat.instructionsFilesLocations": {
        ".github/instructions": true,
        "/tmp/postman-http-request-post-response.instructions.md": true,
        "/tmp/postman-http-request-pre-request.instructions.md": true,
        "/tmp/postman-collections-post-response.instructions.md": true,
        "/tmp/postman-collections-pre-request.instructions.md": true,
        "/tmp/postman-folder-post-response.instructions.md": true,
        "/tmp/postman-folder-pre-request.instructions.md": true
    },
    "remote.autoForwardPortsSource": "hybrid",
    "workbench.colorTheme": "Visual Studio 2019 Dark",
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.chat.agent.thinkingTool": true,
    "dev.containers.defaultExtensions": [
        "eamodio.gitlens",
        "GitHub.copilot"
    ],
    "workbench.editor.showTabs": "none",
    "csharp.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
    "csharp.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
    "csharp.inlayHints.enableInlayHintsForLambdaParameterTypes": true,
    "csharp.inlayHints.enableInlayHintsForTypes": true,
    "dotnet.inlayHints.enableInlayHintsForIndexerParameters": true,
    "dotnet.inlayHints.enableInlayHintsForLiteralParameters": true,
    "dotnet.inlayHints.enableInlayHintsForObjectCreationParameters": true,
    "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
    "dotnet.inlayHints.enableInlayHintsForParameters": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatDifferOnlyBySuffix": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchArgumentName": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchMethodIntent": true
}
